{
  "extends": ["@antfu"],
  "rules": {
    "@typescript-eslint/consistent-type-assertions": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "antfu/if-newline": "off",
    "@stylistic/ts/brace-style": ["off", "1tbs"],
    "curly": ["error", "all"],
    "arrow-parens": ["error", "always"],
    "@stylistic/ts/indent": "off"
  }
}
